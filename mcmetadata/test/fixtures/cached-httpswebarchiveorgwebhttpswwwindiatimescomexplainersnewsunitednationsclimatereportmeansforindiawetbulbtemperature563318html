		"@context":"https://web.archive.org/web/20220721143739/https://schema.org",
		"@type":"NewsArticle",
		"headline":"Explained: What Does The New United Nations 'Alarming' Climate Report Mean For India?",
		"name":"Explained: What Does The New United Nations 'Alarming' Climate Report Mean For India?",
		"datePublished":"2022-03-01T16:31:22+05:30",
		"dateModified":"2022-07-21T19:28:33+05:30",
		"inLanguage":"en",
		"articleSection":"News",
		"keywords":"climate change, united nations, united nations report, climate change in india, india climate change, UN climate report mean for india, ",
		"author":{
			"@type":"Person",
			"name":"Zoya Hussain",
			"url": "https://web.archive.org/web/20220721143739/https://www.indiatimes.com/author/zoya-hussain/5206"
		},
		"thumbnailUrl":"https://web.archive.org/web/20220721143739/https://im.indiatimes.in/content/2022/Mar/5_621dfe7dd7d4b.jpg",
		"image":{
			"@type":"ImageObject",
			"url":"https://web.archive.org/web/20220721143739/https://im.indiatimes.in/content/2022/Mar/5_621dfe7dd7d4b.jpg",
			"width":800,
			"height":420

                window.addEventListener('DOMContentLoaded', () => {
                    function checkUserRealoadAction(activeWindow) {
  var pageAccessByReload = activeWindow.performance.getEntriesByType('navigation').map(function (nav) {
    return nav.type;
  }).includes('reload');
  return pageAccessByReload;
}
                    const refresh = checkUserRealoadAction(window);
                    if (!refresh) {
                        const tags = document.querySelectorAll('section.mod-themes a');
                        function addPositionTag(tag, idexTag) {
  var index = idexTag + 1;
  var position = index <= 9 ? "0".concat(index) : index;
  if (tag) return Object.assign(tag, {
    ctr_brand: "linkTemas_".concat(position),
    ctr_position: "0600".concat(position)
  });
  return true;
}
                        const callback = entries => {
                            entries.forEach((tagElement, i) => {
                                if (tagElement.isIntersecting) {
                                    const { target } = tagElement;

                                    window.dataLayer.push({
                                        event: 'impressionNota',
                                        ctr_brand: target.ctr_brand,
                                        ctr_position: target.ctr_position
                                    });

                                    observer.unobserve(target);
                                }
                            });
                        };

                        const observer = new IntersectionObserver(callback);
                        tags.forEach((tag, i) => {
                            addPositionTag(tag, i);
                            const clickEvent = {
                                event: 'productClickNota',
                                ctr_brand: tag.ctr_brand,
                                ctr_position: tag.ctr_position
                            };
                            tag.addEventListener('click', () => {
                                window.dataLayer.push(clickEvent);
                            });
                            tag.addEventListener('auxclick', () => {
                                window.dataLayer.push(clickEvent);
                            });
                            observer.observe(tag);
                        });
